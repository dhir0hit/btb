{% extends '../_doublenavlayout.twig' %}

{% block body %}
    <div class="user_container">
        <div class="graphic">
            <div>
                <img id="" src="/images/avatar/1.png" alt="avatar">
            </div>
                <h1 id="username">username</h1>

                <h3 id="business_account"></h3>
        </div>

        <div class="detail">
            <div>
                <h3 id="first_name">first_name</h3>
